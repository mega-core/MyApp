<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="100dp" android:height="100dp" android:viewportHeight="100" android:viewportWidth="100">


    <path android:pathData="M 0,0 L 100,100 0,100 z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="50"
                android:endY="100"
                android:startX="50"
                android:startY="50"
                android:type="linear">
                <item
                    android:color="#6727B0"
                    android:offset="0.32" />
                <item
                    android:color="#0683E6"
                    android:offset="1.0" />

            </gradient>

        </aapt:attr>


    </path>



    <path android:pathData="M 0,0 L 100,0 0,100 z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="0"
                android:endY="60"
                android:startX="60"
                android:startY="0"
                android:type="linear">
                <item
                    android:color="#6727B0"
                    android:offset="0.3" />
                <item
                    android:color="#0683E6"
                    android:offset="1.0" />

            </gradient>

        </aapt:attr>


    </path>

    <path
        android:fillColor="#D38006"
        android:pathData="M 100,0 L 60,0 0,60 0,100"/>

    <path android:pathData="M 100,0 L 60,0 0,60 0,100">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="0"
                android:endY="100"
                android:startX="50"
                android:startY="50"
                android:type="linear">
                <item
                    android:color="#D38006"
                    android:offset="0.0" />
                <item
                    android:color="#D5CA51DF"
                    android:offset="1.0" />



            </gradient>


        </aapt:attr>

    </path>
</vector>