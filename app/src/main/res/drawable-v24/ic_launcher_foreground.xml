<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="200dp" android:height="200dp" android:viewportHeight="200" android:viewportWidth="200">


    <path android:pathData="M 50,50 L 150,150 50,150 z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="100"
                android:endY="150"
                android:startX="100"
                android:startY="100"
                android:type="linear">
                <item
                    android:color="#6727B0"
                    android:offset="0.32" />
                <item
                    android:color="#0683E6"
                    android:offset="1.0" />

            </gradient>

        </aapt:attr>


    </path>



    <path android:pathData="M 50,50 L 150,50 50,150 z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="50"
                android:endY="110"
                android:startX="110"
                android:startY="50"
                android:type="linear">
                <item
                    android:color="#6727B0"
                    android:offset="0.3" />
                <item
                    android:color="#0683E6"
                    android:offset="1.0" />

            </gradient>

        </aapt:attr>


    </path>



    <path android:pathData="M 150,50 L 110,50 50,110 50,150">
        <aapt:attr name="android:fillColor">
            <gradient
                android:endX="50"
                android:endY="150"
                android:startX="100"
                android:startY="100"
                android:type="linear">
                <item
                    android:color="#D38006"
                    android:offset="0.0" />
                <item
                    android:color="#D5CA51DF"
                    android:offset="1.0" />



            </gradient>


        </aapt:attr>

    </path>
</vector>